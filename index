    <script>
        var typeData = new Typed(".role", {
            strings: [
                "Software Developer",
                "Full Stack Developer",
                "Web Developer",
                "Backend Developer",
                "Coder",
            ],
            loop: true,
            typeSpeed: 100,
            backSpeed: 80,
            backDelay: 1000,
        });
        function redirectToContact() {
            document.getElementById('contact').scrollIntoView({ behavior: 'smooth' });
        }
        $(document).ready(function () {
            $(".myBtn-1").click(function () {
                $(this).prev().toggle();
                if ($(this).text() == 'Learn More') {
                    $(this).text('Learn Less');
                }
                else {
                    $(this).text('Learn More');
                }
            })
        });
        const scriptURL = 'https://script.google.com/macros/s/AKfycbyfl8w4U_XNJiaJOfX57iSsiuKjxDafRNaSwlp1wSuIY47X3ZWHsZzCJg_I5n5MjE7W/exec'
        const form = document.forms['submit-to-google-sheet']
        const success = document.getElementById('success');
        form.addEventListener('submit', e => {
            e.preventDefault()
            fetch(scriptURL, { method: 'POST', body: new FormData(form) })
                .then(response => {
                    success.innerHTML = "data successfully submit";
                    setTimeout(function () {
                        success.innerHTML = "";
                    }, 5000)
                    form.reset();
                })
                .catch(error => console.error('Error!', error.message))
        })
        window.onload = function () {
            var redirectTo = "index.html";
            if (performance.navigation.type == 1) {
                window.location.href = redirectTo;
            }
        }
    </script>
